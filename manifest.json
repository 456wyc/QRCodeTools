{
  "name": "二维码",
  "version": "1.0",
  "manifest_version": 2,
  "description": "二维码小工具",
  "icons": {
    "16": "icon/icon16.png",
    "32": "icon/icon32.png",
    "64": "icon/icon64.png",
    "128": "icon/icon128.png"
  },
  "permissions": ["tabs", "clipboardRead", "webNavigation", "contextMenus","<all_urls>","activeTab"],
  "browser_action": {
    "default_icon":{
      "16": "icon/icon16.png",
      "32": "icon/icon132.png"
    },
    "default_title": "生成二维码",
    "default_popup": "main.html"
  }
}